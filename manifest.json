{
  "manifest_version": 2,

  "name": "Google account setter",
  "description": "Automatically redirect Google pages to a URL with with the selected user",
  "version": "0.2.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "webNavigation"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
